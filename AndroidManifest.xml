<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scripturesos.tantest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" /> 
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    
    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:logo="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.scripturesos.tantest.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/SherlockBaseTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity 
            android:name="com.scripturesos.tantest.TestActivity"
            android:theme="@style/SherlockBaseTheme"
            android:label="@string/app_name" >
        </activity>
        
        <activity 
            android:name="com.scripturesos.tantest.ContactsActivity"
            android:theme="@style/SherlockBaseTheme"
            android:label="@string/app_name" >
        </activity>
        
        <activity 
            android:name="com.scripturesos.tantest.TestGradeActivity" 
            android:theme="@style/SherlockBaseTheme"
            android:label="@string/app_name" >
        </activity>
        
    </application>

</manifest>